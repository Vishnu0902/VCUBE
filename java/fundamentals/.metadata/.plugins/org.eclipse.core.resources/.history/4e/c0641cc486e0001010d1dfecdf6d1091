package com.practice;

import java.math.BigInteger;
import java.math.BigDecimal;
public class Employee {
	
	{
		 BigInteger bs1 = new BigInteger("5000");
		 BigDecimal da1 = new BigDecimal("5000*8%");
		 BigDecimal pf1 = new BigDecimal("5000*3%");
		 BigDecimal cg1 = new BigDecimal("5000*7%");
		 BigDecimal ms1 = new BigDecimal("bs1+da1-pf1-cg1");
		 BigDecimal as1 = new BigDecimal("ms1*12");
	}

	public static void main(String[] args) {
		Employee e1 = new Employee();
		
	}

}
