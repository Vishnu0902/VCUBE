package com.practice;

import java.math.BigInteger;
import java.math.BigDecimal;
public class Bigobjects {
	BigInteger bi=new BigInteger("123456789987654321");
	BigDecimal bd = new BigDecimal("123456789987654321.123456789987654321");
	

	public static void main(String[] args) {
		Bigobjects t1=new Bigobjects();
		System.out.println("main method started");
		
		System.out.println("BigInteger:"+ t1.bi);
		System.out.println("BigDecimal:"+ t1.bd);
		
		

	}

}
